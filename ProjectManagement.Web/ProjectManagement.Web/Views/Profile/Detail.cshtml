@using Telerik.Web.Mvc.UI;
@model ProjectManagement.Domain.tblMemberDTO
@{
    ViewBag.Title = "Your Profile";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-md-12 ui-sortable">
        <!-- begin panel -->
        <div data-sortable-id="form-stuff-1" class="panel panel-inverse">
            <div class="panel-heading">
                <h4 class="panel-title">Your Profile</h4>
            </div>
            <form class="form-horizontal">
                <div class="panel-body">
                    @Html.HiddenFor(model => model.MemberId)
                    <div class="form-group">
                        <label class="col-md-3 control-label">First Name</label>
                        <div class="col-md-5">
                            <h5>
                                @Model.FirstName
                            </h5>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-3 control-label">Last Name</label>
                        <div class="col-md-5">
                            <h5>
                                @Model.LastName
                            </h5>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-3 control-label">Address</label>
                        <div class="col-md-5">
                            <h5>
                                @Model.Address
                            </h5>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-3 control-label">Email Address</label>
                        <div class="col-md-5">
                            <h5>
                                @Model.EmailAddress
                            </h5>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-3 control-label">Mobile No</label>
                        <div class="col-md-5">
                            <h5>
                                @Model.MobileNo
                            </h5>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-3 control-label"></label>
                        <div class="col-md-5">
                            <button class="btn btn-sm btn-success" type="button" onclick="document.location.href = '@Url.Action("Update", "Profile")'">Update</button>
                        </div>
                    </div>
                </div>
            </form>
        </div>
        <!-- end panel -->
    </div>
</div>
<script src="~/Content/Script/Member.js"></script>
<script type="text/javascript">
    //ActivateSelectedMenu('menuAdminDefinition');
</script>
